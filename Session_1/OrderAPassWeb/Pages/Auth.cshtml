@page
@model OrderAPassWeb.Pages.AuthModel
@{
    ViewData["Title"] = "Авторизация";
}
<form method="post">
    <div class="form-group">
        <label for="exampleInputLogin">Login address</label>
        <input type="text" class="form-control" id="exampleInputlogin" aria-describedby="emailHelp" placeholder="Login" name="login">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword">Email address</label>
        <input type="password" class="form-control" id="exampleInputPassword" aria-describedby="emailHelp" placeholder="Enter password" name="password">
    </div>
    <input type="submit" />
</form>
<style>
    .form-group {
        margin: 10px;
    }

</style>

