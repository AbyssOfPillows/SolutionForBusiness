<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:vm="using:SecurityOfficerTerminal.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="SecurityOfficerTerminal.EditWindow"
        Title="EditWindow"
		x:DataType="vm:EditWindowViewModel">
	<StackPanel>
		<TextBlock Text="Доступ?"></TextBlock>
		<CheckBox Content="{Binding Access}"></CheckBox>
		<TextBox Text="{Binding ArrivalTime}"></TextBox>
		<TextBlock Text="Время отбытия:"></TextBlock>
		<TextBox Text="{Binding DeaprtureTime}"></TextBox>
		<Button Command="{Binding Save}">Сохранить</Button>
	</StackPanel>

</Window>
