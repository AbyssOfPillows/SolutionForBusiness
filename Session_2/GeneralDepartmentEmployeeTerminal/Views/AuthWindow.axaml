<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:vm="using:GeneralDepartmentEmployeeTerminal.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="GeneralDepartmentEmployeeTerminal.AuthWindow"
        Title="AuthWindow"
		x:DataType="vm:AuthWindowViewModel">
	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<TextBlock Text="Код сотрудника:"></TextBlock>
		<TextBox Text="{Binding EnteredCode}"></TextBox>
		<Button Command="{Binding Enter}" Content="Войти" HorizontalAlignment="Center"></Button>
	</StackPanel>
</Window>
