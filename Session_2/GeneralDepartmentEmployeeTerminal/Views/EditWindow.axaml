<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:vm="using:GeneralDepartmentEmployeeTerminal.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="GeneralDepartmentEmployeeTerminal.EditWindow"
        Title="EditWindow"
		x:DataType="vm:EditWindowViewModel">
	<StackPanel>
		<TextBlock Text="Дата визита:"></TextBlock>
		<TextBox Text="{Binding DateVisit}"></TextBox>
		<CheckBox Content="{Binding Approve}">Одобрена?</CheckBox>
		<Button Command="{Binding Save}" Content="Закрыть"></Button>
	</StackPanel>
	
</Window>
